<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="warns">

	<select id="selectList" resultType="Warn">
		SELECT no, title, content, regdate
		FROM warns
		ORDER BY no
	</select>
</mapper>

